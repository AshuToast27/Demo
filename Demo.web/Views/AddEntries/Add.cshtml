
@model Demo.web.Models.ViewModels.AddEntryRequest;
@{
	ViewData["Title"] = "Add Entries";
}

<div class="bg-secondary bg-opacity-10 py-2">
	<div class="container">
		<h1>Add New Entry</h1>
	</div>
</div>

<div class="container py-5">
	<form method="post">
		<div class="mb-3">
			<label class="form-label">First Name</label>
			<input type="text" class="form-control" id="fname" asp-for="Fname"/>
		</div>

		<div class="mb-3">
			<label class="form-label">Last Name</label>
			<input type="text" class="form-control" id="lname" asp-for="Lname" />
		</div>

		<div class="mb-3">
			<label class="form-label">Date of Birth</label>
			<input type="date" class="form-control" id="dob" asp-for="Dob" />
		</div>

		<div class="mb-3">
			<label class="form-label">City</label>
			<input type="text" class="form-control" id="city" asp-for="City" />
		</div>

		<div class="mb-3">
			<label class="form-label">Address</label>
			<input type="text" class="form-control" id="address" asp-for="Address" />
		</div>

		<div class="mb-3">
			<button type="submit" class="btn btn-dark"> Add Entry</button>
		</div>
	</form>
	

</div>
